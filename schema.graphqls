type User {
    id: Int!
    username: String!
    createdAt: String!
    updatedAt: String!
    deletedAt: String!
}

type Query {
    getUser(username: String!): User,
    allUsers: [User!]!
}

type Mutation {
    createUser(username: String!): User
    updateUser(username: String!, newUsername: String!): [Int!]!
    deleteUser(username: String!): Int!
}
